// Test Protocol with Repeated Complex Fields for BitRPC Testing
namespace Example.Protocol

message User {
    string name = 1;
    int32 age = 2;
    string email = 3;
}

message Group {
    string name = 1;
    repeated User members = 2;
    string description = 3;
}

message Organization {
    string name = 1;
    repeated Group groups = 2;
    repeated string departments = 3;
    User leader = 4;
}

service OrganizationService {
    Organization GetOrganization(Organization request);
}